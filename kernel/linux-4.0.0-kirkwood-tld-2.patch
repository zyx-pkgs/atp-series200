diff -Naur a/arch/arm/boot/dts/kirkwood-netgear_stora_ms2000.dts b/arch/arm/boot/dts/kirkwood-netgear_stora_ms2000.dts
--- a/arch/arm/boot/dts/kirkwood-netgear_stora_ms2000.dts	1969-12-31 16:00:00.000000000 -0800
+++ b/arch/arm/boot/dts/kirkwood-netgear_stora_ms2000.dts	2015-04-12 22:24:14.709395341 -0700
@@ -0,0 +1,215 @@
+/dts-v1/;
+
+#include "kirkwood.dtsi"
+#include "kirkwood-6281.dtsi"
+
+/ {
+	model = "Netgear Stora MS2000 / MS2110";
+	compatible = "netgear,stora,ms2000,ms2110", "netgear,stora", "marvell,kirkwood-88f6281", "marvell,kirkwood";
+
+	memory {
+		device_type = "memory";
+		reg = <0x00000000 0x8000000>; /* 128 MB */
+	};
+
+	chosen {
+		bootargs = "console=ttyS0,115200n8 earlyprintk";
+		stdout-path = &uart0;
+	};
+
+	mbus {
+		pcie-controller {
+			status = "okay";
+
+			pcie@1,0 {
+				status = "okay";
+			};
+		};
+	};
+
+	ocp@f1000000 {
+		pinctrl: pin-controller@10000 {
+			pinctrl-0 = < &pmx_fan_tacho >;
+			pinctrl-names = "default";
+
+			pmx_led_green_hdd0: pmx-led-green-hdd0 {
+				marvell,pins = "mpp21";
+				marvell,function = "gpio";
+			};
+			pmx_led_green_hdd1: pmx-led-green-hdd1 {
+				marvell,pins = "mpp20";
+				marvell,function = "gpio";
+			};
+			pmx_led_red_hdd0: pmx-led-red-hdd0 {
+				marvell,pins = "mpp23";
+				marvell,function = "gpio";
+			};
+			pmx_led_red_hdd1: pmx-led-red-hdd1 {
+				marvell,pins = "mpp22";
+				marvell,function = "gpio";
+			};
+			pmx_led_blue_power0: pmx-led-blue-power0 {
+				marvell,pins = "mpp31";
+				marvell,function = "gpio";
+			};
+			pmx_led_blue_power1: pmx-led-blue-power1 {
+				marvell,pins = "mpp32";
+				marvell,function = "gpio";
+			};
+			pmx_power_button: pmx-power-button {
+				marvell,pins = "mpp36";
+				marvell,function = "gpio";
+			};
+			pmx_reset_button: pmx-reset-button {
+				marvell,pins = "mpp37";
+				marvell,function = "gpio";
+			};
+			pmx_poweroff: pmx-poweroff {
+				marvell,pins = "mpp40";
+				marvell,function = "gpio";
+			};
+			pmx_fan_tacho: pmx-fan-tacho {
+				marvell,pins = "mpp41";
+				marvell,function = "gpio";
+			};
+		};
+
+		clocks {
+			pcf8563_clk: pcf8563-oscillator {
+				compatible = "fixed-clock";
+				#clock-cells = <0>;
+				clock-frequency = <32768>;
+			};
+		};
+
+		i2c@11000 {
+			status = "okay";
+
+			lm75: lm75@48 {
+				compatible = "national,lm75";
+				reg = <0x48>;
+			};
+
+			pcf8563: pcf8563@51 {
+				compatible = "nxp,pcf8563";
+				reg = <0x51>;
+			};
+		};
+
+		serial@12000 {
+			status = "okay";
+		};
+
+		sata@80000 {
+			nr-ports = <2>;
+			status = "okay";
+		};
+
+		ehci@50000 {
+			status = "okay";
+		};
+
+	};
+
+	gpio-leds {
+		compatible = "gpio-leds";
+		pinctrl-0 = <	&pmx_led_blue_power0 &pmx_led_blue_power1
+				&pmx_led_green_hdd0 &pmx_led_green_hdd1
+				&pmx_led_red_hdd0 &pmx_led_red_hdd1 >;
+		pinctrl-names = "default";
+
+		led@1 {
+			label = "stora:blue:power0";
+			gpios = <&gpio1 31 GPIO_ACTIVE_HIGH>;
+			default-state = "keep";
+		};
+		led@2 {
+			label = "stora:blue:power1";
+			gpios = <&gpio1 32 GPIO_ACTIVE_HIGH>;
+			default-state = "keep";
+		};
+		led@3 {
+			label = "stora:green:hdd0";
+			gpios = <&gpio1 21 GPIO_ACTIVE_HIGH>;
+		};
+		led@4 {
+			label = "stora:green:hdd1";
+			gpios = <&gpio1 20 GPIO_ACTIVE_HIGH>;
+		};
+		led@5 {
+			label = "stora:red:hdd0";
+			gpios = <&gpio1 23 GPIO_ACTIVE_HIGH>;
+		};
+		led@6 {
+			label = "stora:red:hdd1";
+			gpios = <&gpio1 22 GPIO_ACTIVE_HIGH>;
+		};
+	};
+
+	gpio_keys {
+		compatible = "gpio-keys";
+		#address-cells = <1>;
+		#size-cells = <0>;
+		pinctrl-0 = < &pmx_power_button &pmx_reset_button >;
+		pinctrl-names = "default";
+
+		button@1 {
+			label = "Power button";
+			linux,code = <KEY_POWER>;
+			gpios = <&gpio1 36 GPIO_ACTIVE_LOW>;
+			debounce-interval = <100>;
+		};
+		button@2 {
+			label = "Reset button";
+			linux,code = <KEY_RESTART>;
+			gpios = <&gpio0 37 GPIO_ACTIVE_LOW>;
+			debounce-interval = <100>;
+		};
+	};
+
+	gpio-poweroff {
+		compatible = "gpio-poweroff";
+		pinctrl-0 = <&pmx_poweroff>;
+		pinctrl-names = "default";
+		gpios = <&gpio0 40 GPIO_ACTIVE_LOW>;
+	};
+};
+
+&nand { 
+	status = "okay";
+	pinctrl-0 = <&pmx_nand>;
+	pinctrl-names = "default";
+	chip-delay = <35>;
+
+	partition@0 {
+		label = "uboot";
+		reg = <0x0000000 0x0100000>;
+		read-only;
+	};
+
+	partition@100000 {
+		label = "uImage";
+		reg = <0x0100000 0x0400000>;
